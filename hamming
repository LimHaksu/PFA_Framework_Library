// DFA
digraph {
	0 [label=0 shape=circle]
	1 [label=1 shape=circle]
	2 [label=2 shape=doublecircle]
	3 [label=3 shape=circle]
	0 -> 1 [label=a]
	1 -> 3 [label=a]
	2 -> 3 [label=a]
	3 -> 3 [label=a]
	0 -> 3 [label=b]
	1 -> 2 [label=b]
	2 -> 3 [label=b]
	3 -> 3 [label=b]
	node [shape=none]
	""
	"" -> 0
}
