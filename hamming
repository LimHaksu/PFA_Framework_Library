	0 [label=0 shape=circle]
	1 [label=1 shape=circle]
	2 [label=2 shape=doublecircle]
	3 [label=3 shape=circle]
	4 [label=4 shape=doublecircle]
	5 [label=5 shape=doublecircle]
	6 [label=6 shape=circle]
	0 -> 1 [label=a]
	1 -> 4 [label="∑\\b"]
	2 -> 6 [label="∑"]
	3 -> 5 [label="∑\\b"]
	4 -> 6 [label="∑"]
	5 -> 6 [label="∑"]
	6 -> 6 [label="∑"]
	0 -> 3 [label="∑\\a"]
	1 -> 2 [label=b]
	3 -> 4 [label=b]
	node [shape=none]
	""
	"" -> 0
}
