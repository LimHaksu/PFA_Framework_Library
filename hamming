// DFA
digraph {
	0 [label=0 shape=circle]
	1 [label=1 shape=circle]
	2 [label=2 shape=circle]
	3 [label=3 shape=circle]
	4 [label=4 shape=doublecircle]
	5 [label=5 shape=circle]
	0 -> 5 [label="∑\\d"]
	1 -> 5 [label="∑\\d"]
	2 -> 5 [label="∑\\f"]
	3 -> 5 [label="∑\\d"]
	4 -> 5 [label="∑"]
	5 -> 5 [label="∑"]
	0 -> 1 [label=d]
	1 -> 2 [label=d]
	3 -> 4 [label=d]
	2 -> 3 [label=f]
	node [shape=none]
	""
	"" -> 0
}
